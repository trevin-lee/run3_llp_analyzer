{
    "folders": [
        {
            "name": "run3_llp_analyzer",
            "path": "."
        },
        {
            "name": "CMSSW_BASE",
            "path": "${env:CMSSW_BASE}/src"
        }
    ],
    "settings": {
        "C_Cpp.default.includePath": [
            "${workspaceFolder}/**",
            "${env:CMSSW_BASE}/src/**",
            "${env:CMSSW_RELEASE_BASE}/src/**",
            "${env:ROOTSYS}/include/**"
        ],
        "C_Cpp.default.defines": [
            "PROJECT_NAME=\"run3_llp_analyzer\"",
            "PROJECT_VERSION=\"1.0.0\"",
            "GNU_GCC",
            "__GNUC__",
            "__cplusplus=201703L"
        ],
        "C_Cpp.default.compilerPath": "/usr/bin/g++",
        "C_Cpp.default.cppStandard": "c++17",
        "C_Cpp.default.cStandard": "c17",
        "C_Cpp.default.intelliSenseMode": "linux-gcc-x64"
    },
    "extensions": {
        "recommendations": [
            "ms-vscode.cpptools",
            "ms-vscode.cpptools-extension-pack",
            "ms-vscode.cmake-tools",
            "ms-python.python",
            "eamodio.gitlens"
        ]
    }
} 